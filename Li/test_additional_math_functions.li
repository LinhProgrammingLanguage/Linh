import math

print("Testing additional mathematical functions:")

print("\nTesting math.pow():")
print(math.pow(2, 3))        // 8
print(math.pow(3, 2))        // 9
print(math.pow(2, 0.5))      // sqrt(2)
print(math.pow(0, 5))        // 0
print(math.pow(5, 0))        // 1

print("\nTesting math.sqrt():")
print(math.sqrt(9))          // 3
print(math.sqrt(2))          // sqrt(2)
print(math.sqrt(0))          // 0
print(math.sqrt(16))         // 4

print("\nTesting math.cbrt():")
print(math.cbrt(27))         // 3
print(math.cbrt(8))          // 2
print(math.cbrt(0))          // 0
print(math.cbrt(-27))        // -3

print("\nTesting math.exp():")
print(math.exp(0))           // 1
print(math.exp(1))           // e
print(math.exp(-1))          // 1/e

print("\nTesting math.expm1():")
print(math.expm1(0))         // 0
print(math.expm1(1))         // e-1
print(math.expm1(-1))        // 1/e-1

print("\nTesting math.log():")
print(math.log(1))           // 0
print(math.log(math.e))      // 1
print(math.log(10))          // ln(10)

print("\nTesting math.log1p():")
print(math.log1p(0))         // 0
print(math.log1p(1))         // ln(2)
print(math.log1p(-0.5))      // ln(0.5)

print("\nTesting math.log10():")
print(math.log10(1))         // 0
print(math.log10(10))        // 1
print(math.log10(100))       // 2

print("\nTesting math.log2():")
print(math.log2(1))          // 0
print(math.log2(2))          // 1
print(math.log2(8))          // 3 