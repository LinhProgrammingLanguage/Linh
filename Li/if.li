var a = 2
switch (a) {
    case 1:
        print("switch: a = 1")
        break
    case 2:
        print("switch: a = 2")
        break
    default:
        print("switch: default")
        break
}

var x = 10
if (x < 5) {
    print("if: x < 5")
} else if (x < 15) {
    print("else if: x < 15")
} else {
    print("else: x >= 15")
}