import math

// Test math functions
print("Testing math.abs() function:")
print(math.abs(-5))
print(math.abs(-3.14))
print(math.abs(0))
print(math.abs(42))

print("\nTesting math.ceil() function:")
print(math.ceil(3.14))
print(math.ceil(3.0))
print(math.ceil(-3.14))
print(math.ceil(-3.0))

print("\nTesting math.floor() function:")
print(math.floor(3.14))
print(math.floor(3.0))
print(math.floor(-3.14))
print(math.floor(-3.0))

print("\nTesting math.round() function:")
print(math.round(3.14))
print(math.round(3.5))
print(math.round(-3.14))
print(math.round(-3.5))

print("\nTesting math.trunc() function:")
print(math.trunc(3.14))
print(math.trunc(3.0))
print(math.trunc(-3.14))
print(math.trunc(-3.0))

print("\nTesting math constants:")
print(math.pi)
print(math.e)
print(math.tau) 